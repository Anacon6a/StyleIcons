    <script type="text/javascript">
        $(function () {
            if (typeof ProductPage != "undefined") {
                ProductPage.init($('.js-product-page'));
            }
            var x = new LeftMenu($('#layerDescription'), {
                notSetStyleOnFix: true,
                fixTop: 55,
                noCache: true,
                fastInit: true
            });
            window.afterLoadDeliveryLocation = function () {
                setTimeout(function () {
                    x.onScroll();
                }, 100);
            }
            var defTop = parseInt($('.js-scroll-product .characteristics').css('top'));
            var defScroll = $(window).scrollTop();
            var fixPos = 20;

            function fix_resize() {
                var el = $('.js-scroll-product .characteristics');
                var position = 0;
                if (el.children().height() + defTop > $(window).height()) {
                    var top = parseInt(el.children().children().css('top'));
                    top = ((isNaN(top)) ? 0 : top);
                    position = (top - ($(window).scrollTop() - defScroll) < 0) ? top - ($(window).scrollTop() - defScroll) : 0;
                    position = (position > $(window).height() - (el.children().height() + defTop + fixPos)) ? position : $(window).height() - (el.children().height() + defTop + fixPos);
                }
                defScroll = $(window).scrollTop();
                var fixStopScroll = $('.fixScroll').offset();
                if (el.height() <= (defScroll + el.children().height())) {
                    position = position + (el.height() - (defScroll + el.children().height()));
                }
                el.children().children().css('top', position + 'px');

            }

            $(document).scroll(function () {
                fix_resize();
            });
            $(window).resize(function () {
                fix_resize();
            });
            $('.js-subscribe-btn').click(function () {
                modal({html: $('.js-subscribe .js-body-p').html()}).start();

                return false;
            });
            $('body').on('submit', '#subscribe-form', function () {
                var $form = $(this);
                var email = $form.find('input[name=email]').val();
                var productId = $form.find('input[name=product_id]').val();
                var btn = $form.find('button');
                var token = $form.find('input[name=_token]').val();
                var sectionId = $form.find('input[name=section_id]').val();
                var $error = $form.find('.js-error-text');
                $error.html('');
                if (email.trim() != '' && productId) {
                    btn.attr('disabled', 'disabled');
                    btn.text(btn.data('wait'));
                    var action = $form.attr('action');
                    var params = {
                        'email': email,
                        'product_id': productId,
                        'section_id': sectionId,
                        '_token': token
                    };
                    $.post(action, params, function (data) {
                        if (data.text) {
                            $form.hide();
                            $form.parent().find('.js-ok').show();
                            setTimeout(function () {
                                modal.close();
                            }, 2000);
                            $('.js-subscribe-btn').attr('disabled', 'disabled').html(data.text);
                        } else {
                            $form.closest('.modal').modal('hide');
                        }
                    }).error(function (data) {
                        var error = [];
                        if (data.responseJSON) {
                            var d = data.responseJSON;
                            for (var key in d) {
                                if (d.hasOwnProperty(key)) {
                                    if (typeof d[key] == 'string') {
                                        error.push(d[key]);
                                    } else if (typeof d[key] && d[key].length) {
                                        for (var i = 0; i < d[key].length; i++) {
                                            error.push(d[key][i]);
                                        }
                                    }
                                }
                            }
                        }
                        if (error.length == 0) {
                            error.push($form.data('error'));
                        }
                        $error.html(error.join('<br>'));
                    }).always(function () {
                        btn.attr('disabled', false);
                        btn.text(btn.data('normal'));
                    });

                } else {
                    $form.find('input[name=email]').addClass('error').focus();
                    setTimeout(function () {
                        $form.find('input[name=email]').removeClass('error');
                    }, 500);
                }
                return false;
            })
            //modal({html:'<div style="position:relative; min-width:220px;">'+$banner.find('.js-skip').html()+'</div>'}).start();
        })
    </script>
